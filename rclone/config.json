{
  "name": "Rclone",
  "version": "1.0.0",
  "slug": "rclone",
  "description": "Rclone integration for Home Assistant",
  "arch": ["amd64", "armv7", "armhf", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "image": "rclone/rclone:latest",
